page {
  background-color: #F7F7F9;
}

.container {
  display: flex;
  flex-flow: column;
}

.staticContainer {
  display: flex;
  flex-flow: column;
  position: fixed;
  width: 100%;
  background-color: #F7F7F9;
  z-index: 999;
}

.tabContainer {
  width: 100%;
  height: 96rpx;
  flex-flow: row;
  display: flex;
  border-top: 1rpx solid #EAEAEA;
  background-color: white;
}

.tabTitle {
  align-self: center;
  text-align: center;
}

.title1 {
  margin-left: 32rpx;
}

.title2 {
  margin-left: 40rpx;
  margin-right: 40rpx;
}

.countContainer {
  width: 686rpx;
  height: 170rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  margin-top: 32rpx;
  margin-bottom: 32rpx;
  display: flex;
  flex-flow: row;
  align-self: center;
}

.titleContainer {
  display: flex;
  flex-flow: column;
  align-self: center;
}

.titleBottom {
  width: 40rpx;
  height: 8rpx;
  align-self: center;
  background-color: #63CEE4;
  margin-top: 10rpx;
}

.childContainer {
  display: flex;
  flex-flow: column;
  align-self: center;
  flex-grow: 1;
  align-items: center;
}

.child {
  font-size: 26rpx;
  color: #858C96;
}

.childCount {
  font-size: 32rpx;
  color: #212832;
  margin-top: 15rpx;
}

.listContainer {
  width: 686rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  display: flex;
  flex-flow: column;
  margin-top: 330rpx;
}

.listItem {
  display: flex;
  flex-flow: column;
}

.dataContainer {
  height: 144rpx;
  display: flex;
  flex-flow: row;
}

.headItem {
  width: 76rpx;
  height: 76rpx;
  align-self: center;
  margin-left: 26rpx;
  border-radius: 38rpx;
}

.userContainer {
  display: flex;
  flex-flow: column;
  align-self: center;
  margin-left: 15rpx;
}

.name {
  font-size: 28rpx;
  color: #212832;
}

.fansCount {
  font-size: 26rpx;
  color: #ADB4BE;
}

.partnerContainer {
  display: flex;
  flex-flow: column;
  right: 0;
  margin-right: 56rpx;
  align-self: center;
  position: absolute;
}

.bottomLine {
  width: 638rpx;
  height: 1rpx;
  align-self: center;
  background-color: #EAEAEA;
}

.noMore {
  font-size: 26rpx;
  color: #858C96;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.vLine {
  width: 1px;
  height: 82px;
  align-self: center;
  background-color: #F7F7F9;
}